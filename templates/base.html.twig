<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
        
    </head>
    <body>
        <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
            <div class="container-fluid">
                <a class="navbar-brand" href="{{ path('home') }}">MySite</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
                        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav">
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('home') }}">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('about') }}">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('lucky') }}">Lucky</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('api') }}">API</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('report') }}">Report</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ path('pig_init_get') }}">Pig game (init)</a>
                        </li>

                    </ul>
                </div>
            </div>
        </nav>


        {% block body %}{% endblock %}
    </body>
</html>
